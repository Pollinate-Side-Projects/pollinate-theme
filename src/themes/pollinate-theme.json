{
    "name": "Pollinate Theme",
    "type": "dark",
    "colors": {
      "editor.background": "#0F111A",
      "editor.foreground": "#f4f4da",
      "editor.lineNumberForeground": "#6B7280",
      "editor.selectionBackground": "#f15a2544",
      "editor.selectionForeground": "#f4f4da",
      "editor.lineHighlightBackground": "#1a1d2b",
      "editor.wordHighlightBackground": "#f15a2522",
      "editor.findMatchBackground": "#f15a2566",
      "editor.findMatchHighlightBackground": "#f15a2533",
      "sideBar.background": "#0B0D14",
      "sideBar.foreground": "#f4f4da",
      "sideBarSectionHeader.background": "#151820",
      "sideBarSectionHeader.foreground": "#f15a25",
      "list.activeSelectionBackground": "#1a1f2b",
      "list.activeSelectionForeground": "#f4f4da",
      "list.hoverBackground": "#16182466",
      "list.focusBackground": "#1a1f2b",
      "activityBar.background": "#0B0D14",
      "activityBar.foreground": "#f4f4da",
      "activityBar.activeBorder": "#f15a25",
      "activityBarBadge.background": "#f15a25",
      "activityBarBadge.foreground": "#0B0D14",
      "statusBar.background": "#0B0D14",
      "statusBar.foreground": "#f4f4da",
      "statusBarItem.hoverBackground": "#1a1d2b",
      "statusBarItem.prominentBackground": "#f15a25",
      "statusBarItem.prominentForeground": "#0B0D14",
      "button.background": "#f15a25",
      "button.foreground": "#0B0D14",
      "button.hoverBackground": "#ff7a4d",
      "inputBox.background": "#16182466",
      "inputBox.foreground": "#f4f4da",
      "inputBox.border": "#f15a2544",
      "dropdown.background": "#16182466",
      "dropdown.foreground": "#f4f4da",
      "dropdown.border": "#f15a2544",
      "terminal.ansiBlack": "#0B0D14",
      "terminal.ansiRed": "#f15a25",
      "terminal.ansiGreen": "#98C379",
      "terminal.ansiYellow": "#f4f4da",
      "terminal.ansiBlue": "#61AFEF",
      "terminal.ansiMagenta": "#C678DD",
      "terminal.ansiCyan": "#56B6C2",
      "terminal.ansiWhite": "#f4f4da",
      "editorBracketMatch.background": "#f15a2544",
      "editorBracketMatch.border": "#f15a25"
    },
    "tokenColors": [
      {
        "scope": ["comment"],
        "settings": {
          "foreground": "#6B7280",
          "fontStyle": "italic"
        }
      },
      {
        "scope": ["string"],
        "settings": {
          "foreground": "#98C379"
        }
      },
      {
        "scope": ["string.regexp"],
        "settings": {
          "foreground": "#f15a25"
        }
      },
      {
        "scope": ["number"],
        "settings": {
          "foreground": "#D19A66"
        }
      },
      {
        "scope": ["built-in", "constant.language"],
        "settings": {
          "foreground": "#f15a25"
        }
      },
      {
        "scope": ["constant.character.escape"],
        "settings": {
          "foreground": "#56B6C2"
        }
      },
      {
        "scope": ["variable"],
        "settings": {
          "foreground": "#f4f4da"
        }
      },
      {
        "scope": ["variable.parameter"],
        "settings": {
          "foreground": "#D7D7D7",
          "fontStyle": "italic"
        }
      },
      {
        "scope": ["invalid"],
        "settings": {
          "foreground": "#f15a25",
          "background": "#2a0f0f"
        }
      },
      {
        "scope": ["invalid.deprecated"],
        "settings": {
          "foreground": "#C678DD",
          "background": "#2a0f1f"
        }
      },
      {
        "scope": ["keyword", "storage.type"],
        "settings": {
          "foreground": "#f15a25"
        }
      },
      {
        "scope": ["keyword.operator"],
        "settings": {
          "foreground": "#56B6C2"
        }
      },
      {
        "scope": ["keyword.other.special-method"],
        "settings": {
          "foreground": "#61AFEF"
        }
      },
      {
        "scope": ["storage"],
        "settings": {
          "foreground": "#f15a25"
        }
      },
      {
        "scope": ["storage.type.class.jsdoc"],
        "settings": {
          "foreground": "#98C379"
        }
      },
      {
        "scope": ["source.java storage.type"],
        "settings": {
          "foreground": "#f4f4da"
        }
      },
      {
        "scope": ["entity.name.method.js"],
        "settings": {
          "foreground": "#61AFEF"
        }
      },
      {
        "scope": ["entity.name.class", "entity.name.type.class"],
        "settings": {
          "foreground": "#E5C07B"
        }
      },
      {
        "scope": ["entity.name.function"],
        "settings": {
          "foreground": "#61AFEF"
        }
      },
      {
        "scope": ["entity.name.tag"],
        "settings": {
          "foreground": "#f15a25"
        }
      },
      {
        "scope": ["entity.other.attribute-name"],
        "settings": {
          "foreground": "#E5C07B"
        }
      },
      {
        "scope": ["entity.other.attribute-name.html"],
        "settings": {
          "foreground": "#E5C07B"
        }
      },
      {
        "scope": ["entity.other.attribute-name.html.vue"],
        "settings": {
          "foreground": "#E5C07B"
        }
      },
      {
        "scope": ["entity.other.attribute-name.css"],
        "settings": {
          "foreground": "#E5C07B"
        }
      },
      {
        "scope": ["punctuation.definition.string.begin", "punctuation.definition.string.end"],
        "settings": {
          "foreground": "#98C379"
        }
      },
      {
        "scope": ["punctuation.separator.inheritance.php"],
        "settings": {
          "foreground": "#f15a25"
        }
      },
      {
        "scope": ["punctuation.definition.tag"],
        "settings": {
          "foreground": "#f15a25"
        }
      },
      {
        "scope": ["markup.inline.raw"],
        "settings": {
          "foreground": "#E5C07B"
        }
      },
      {
        "scope": ["markup.deleted", "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"],
        "settings": {
          "foreground": "#f15a25"
        }
      },
      {
        "scope": ["markup.inserted"],
        "settings": {
          "foreground": "#98C379"
        }
      },
      {
        "scope": ["markup.changed"],
        "settings": {
          "foreground": "#E5C07B"
        }
      },
      {
        "scope": ["punctuation.section.embedded"],
        "settings": {
          "foreground": "#f15a25"
        }
      },
      {
        "scope": ["meta.embedded.block.html"],
        "settings": {
          "foreground": "#f4f4da"
        }
      }
    ]
}
  