{
    "name": "My Cute Theme",
    "type": "dark",
    "colors": {
      "editor.background": "#0F111A",
      "editor.foreground": "#E6E6E6",
      "sideBar.background": "#0B0D14",
      "sideBar.foreground": "#D7D7D7",
      "activityBar.background": "#0B0D14",
      "statusBar.background": "#0B0D14",
      "list.activeSelectionBackground": "#1A1F2B",
      "list.hoverBackground": "#141826"
